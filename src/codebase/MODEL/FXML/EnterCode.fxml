<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: cedee0;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="codebase.MODEL.EnterCodeController">
   <children>
      <JFXButton fx:id="close" layoutX="228.0" layoutY="14.0" onAction="#handleClose" text="Close">
         <font>
            <Font name="Menlo Regular" size="11.0" />
         </font></JFXButton>
      <Label layoutX="14.0" layoutY="48.0" prefHeight="16.0" prefWidth="280.0" text="A Unique Code has been sent to your email">
         <font>
            <Font name="Menlo Regular" size="11.0" />
         </font></Label>
      <JFXTextField fx:id="code" alignment="CENTER" labelFloat="true" layoutX="107.0" layoutY="87.0" promptText="Code">
         <font>
            <Font name="Menlo Regular" size="13.0" />
         </font></JFXTextField>
      <JFXButton fx:id="submit" layoutX="228.0" layoutY="134.0" onAction="#handleSubmit" text="Submit">
         <font>
            <Font name="Menlo Regular" size="11.0" />
         </font></JFXButton>
   </children>
</AnchorPane>
