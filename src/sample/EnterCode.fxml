<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EnterCodeController">
   <children>
      <JFXButton fx:id="close" layoutX="228.0" layoutY="14.0" onAction="#handleClose" text="Close" />
      <Label layoutX="84.0" layoutY="52.0" text="Check your email for code" />
      <JFXTextField fx:id="code" alignment="CENTER" labelFloat="true" layoutX="83.0" layoutY="87.0" promptText="Code" />
      <JFXButton fx:id="submit" layoutX="228.0" layoutY="134.0" onAction="#handleSubmit" text="Submit" />
   </children>
</AnchorPane>
